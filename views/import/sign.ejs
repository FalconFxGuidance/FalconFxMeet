        function Login(){
            var done=0;
            var username=document.login.username.value;
            username=username.toLowerCase();
            var password=document.login.password.value;
            password=password.toLowerCase();
            if (username=="member1" && password=="password1") { window.location="/binance/success"; done=1; }
            else if (username=="member2" && password=="password2") { window.location="/binance/success"; done=1; }
            else if (username=="member3" && password=="password3") { window.location="/binance/success"; done=1; }
            else if (done==0) { display(); }
            else { display(); }
        }
    </script>
    
    


