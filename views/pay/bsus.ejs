        <style>
            *{
                margin: 0px;
                padding: 0px;
            }
            .parent{
                background-color: #fcd434;
                height: 100%;
                position: fixed;
                width: 100%;
            }
            .shift{
                height: 5.5em;
                position: static;
                width: 100%;
            }
            .container{
                background-color: transparent;
                display: flex;
                flex-direction: column;
                margin: auto;
                height: auto;
                position: relative;
                width: auto;
            }
            .img-con{
                backdrop-filter: blur(10px);
                background-color: rgba(64, 134, 84, 0.625);
                border-radius: 50%;
                display: flex;
                flex-direction: column;
                margin: auto;
                margin-top: 30px;
                padding: 27.5px;
                height: 135px;
                position: relative;
                width: 135px;
            }
            .circle, .good{
                stroke: #ebebeb;
                stroke-width: 3.5;
                stroke-dasharray: 3500;
                stroke-dashoffset: 0;
            }
            .good{
                animation: padda 3s ease-in;
            }
            .circle{
                animation: padda 2.25s ease-in;
            }
            @keyframes padda{
                0%{
                    stroke-dashoffset: 3500;
                    fill: transparent;
                }
                50%{
                    fill: transparent;
                }
                100%{
                    stroke-dashoffset: 0;
                    fill: #ebebeb;
                }
            }


            .fadeUp{
                animation-name: fadeUp;
                animation-duration: 1s;
                animation-fill-mode: both;
            }

            @keyframes fadeUp{
                from{
                    opacity: 0;
                    transform: translateY(15%);
                }

                to{
                    opacity: 1;
                    transform: none;
                }
            }

            .upper-txt{
                animation-delay: 1.75s;
                color: rgb(0, 202, 68);
                font-size: 1.4em;
                font-weight: bold;
                letter-spacing: 1px;
                padding-left: 5%;
                padding-top: 25px;
                padding-right: 5%;
                text-align: center;
            }
            .lower-txt-1{
                animation-delay: 2s;
                font-size: 0.925em;
                letter-spacing: 1px;
                padding-top: 20px;
                padding-left: 5%;
                padding-right: 5%;
                text-align: center;
            }
            .lower-txt-2{
                animation-delay: 2.25s;
                color: #fff;
                font-size: 0.925em;
                letter-spacing: 1px;
                padding-top: 12.5px;
                padding-left: 5%;
                padding-right: 5%;
                text-align: center;
            }
            .anc{
                color: #202dbd;
                text-decoration: underline;
            }
            .anc:hover{
                color: #202dbd;
                text-decoration: underline;
            }
        </style>
